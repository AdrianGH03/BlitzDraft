{
    "version": 2,
    "headers": [
      {
        "source": "/(.*).(jpg|jpeg|png|gif)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, max-age=31536000"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }